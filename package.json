{
  "name": "api_rest9b",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node scripts/start-with-checks.js",
    "start:simple": "node server.js",
    "start:docker": "node scripts/docker-start.js",
    "dev": "nodemon server.js",
    "health": "node scripts/health-check.js",
    "health:watch": "node scripts/health-check.js --watch",
    "docker:build": "docker-compose build --no-cache",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose down && docker-compose up -d",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "docker:test": "node scripts/test-docker.js",
    "docker:full": "npm run docker:build && npm run docker:up && npm run docker:test",
    "deploy": "bash scripts/deploy.sh",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down"
  },
  "keywords": [],
  "author": "Juan de Dios Valero Casillas",
  "license": "ISC",
  "description": "API REST para Cl√≠nica Veterinaria Patitas Felices",
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
